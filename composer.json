{
    "name": "lloricode/spatie-laravel-permission-extended",
    "description": "Extending spatie's laravel-permission Functions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lloric Mayuga Garcia",
            "email": "lloricode@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "spatie/laravel-permission": "^2.9"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit" : "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lloricode\\SpatieLaravelPermissionExtended\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloricode\\SpatieLaravelPermissionExtended\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lloricode\\SpatieLaravelPermissionExtended\\Providers\\SpatieLaravelPermissionExtendedProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
